<UserControl x:Class="UtilityWpf.DemoAppCore.View.ChartUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UtilityWpf.DemoAppCore.View"
             xmlns:chart="clr-namespace:UtilityWpf.Chart;assembly=UtilityWpf.Chart"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <chart:OxyChartSelector x:Name="Selector" Id="First" Items="{StaticResource Characters}"></chart:OxyChartSelector>
    </Grid>
</UserControl>
