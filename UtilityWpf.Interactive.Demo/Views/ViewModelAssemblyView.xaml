<UserControl x:Class="UtilityWpf.Interactive.Demo.ViewModelAssemblyView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:view1="clr-namespace:UtilityWpf.Interactive.View.Controls;assembly=UtilityWpf.Interactive.View" xmlns:reactiveui="http://reactiveui.net"
             xmlns:conv="clr-namespace:UtilityWpf.Converter;assembly=UtilityWpf" 
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <view1:MasterDetailView  x:Name="Main_MasterDetailView" UseDataContext="True" >
            
            <reactiveui:ViewModelViewHost VerticalContentAlignment="Stretch" 
                                          HorizontalContentAlignment="Stretch"
                                          ViewModel="{Binding Object,Converter={x:Static conv:DefaultConverter.Instance}}">
                
            </reactiveui:ViewModelViewHost>
        </view1:MasterDetailView>
    </Grid>
</UserControl>
